
.updateList {
    margin: 30rpx;
    padding-bottom: 30rpx;
    border-bottom: 1rpx solid #e5e5e5;
}
.version {
    color: #353535;
    font-size: 50rpx;
    line-height: 48rpx;
}
.date {
    font-size: 30rpx;
    color: #888;
    line-height: 50rpx;
    margin-bottom: 20rpx;
}
.itemList {
    text-align: left;
    font-size: 30rpx;
    color: #353535;
    line-height: 70rpx;
}
.author {
    position: fixed;
    bottom: 0;
    width: 100%;
    color: #888;
    font-size: 40rpx;
    text-align: center;
    margin-bottom: 30rpx;
}
.author image {
    display: block;
    width: 500rpx;
    height: 150rpx;
    margin: 0 auto;
    margin-bottom: 50rpx;
}
.author text {
    display: block;
    line-height: 40rpx;
}


